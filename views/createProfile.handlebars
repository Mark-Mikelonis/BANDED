<form
  ref='uploadForm' 
  id='uploadForm' 
  action='assets/images/users'
  method='post' 
  encType="multipart/form-data">
  <div class="form-group">
    <label for="name">Username: </label>
    <input type="text" class="form-control" id="name" placeholder="username">
  </div>
  <div class="form-group">
    <label for="name">Location: </label>
    <input type="text" class="form-control" id="location" placeholder="Oakland, CA">
  </div>
  <div class="form-group">
    <label for="bandorfan">Are you a Band or Fan?</label>
    <select class="form-control" id="bandOrFan">
      <option value="fan">Fan</option>
      <option value="band">Band</option>
    </select>
  </div>
  <div class="form-group" id="bandForm">
    <label for="name">Genre: </label>
    <input type="text" class="form-control" id="name" placeholder="username">
  </div>
  <div class="form-group" id="bandForm">
    <label for="bio">Bio: </label>
    <textarea class="form-control" id="bio" rows="3"></textarea>
  </div>
  <div class="form-group" id="bandForm">
    <label for="profileImg">Profile Image: </label>
    <input name="profileImg" type="file" class="form-control-file" aria-describedby="fileHelp">
    <small id="fileHelp" class="form-text text-muted"></small>
  </div>
  <button type="submit" value="Upload!" class="btn btn-primary" id="profSubmit">Submit</button>
</form>